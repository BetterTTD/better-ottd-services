{
  "Serilog": {
    "SeqServerUrl": "http://localhost:5341",
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] [AdminClient] {Message:lj} {NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      },
      { "Name": "Debug" }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "OpenTTD.StateService"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RedisConnectionString": "localhost:6379",
    "ServerDbConnectionString": "Host=localhost;Port=5432;Database=OpenTTDDB;Username=sa;Password=p@ssw0rd;"
  }
}
